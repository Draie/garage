

{% block title %}Auto{% endblock %}

{% block body %}
    <h1>Auto</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ auto.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ auto.Name }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ auto.price }}</td>
            </tr>
            <tr>
                <th>Img</th>
                <td>{{ auto.img ? auto.img|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>AutoDate</th>
                <td>{{ auto.autoDate ? auto.autoDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Kilometrage</th>
                <td>{{ auto.kilometrage }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ auto.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_auto_index') }}">back to list</a>

    <a href="{{ path('app_auto_edit', {'id': auto.id}) }}">edit</a>

    {{ include('auto/_delete_form.html.twig') }}
{% endblock %}
